<p id="notice"><%= notice %></p>

<h1>Listing Societies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Pincode</th>
      <th>Est date</th>
      <th>Start date</th>
      <th>Builder</th>
      <th>Area</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @society_all.each do |society| %>
      <tr>
        <td><%= society.name %></td>
        <td><%= society.address %></td>
        <td><%= society.pincode %></td>
        <td><%= society.est_date %></td>
        <td><%= society.start_date %></td>
        <td><%= society.builder %></td>
        <td><%= society.area %></td>
        <td><%= link_to 'Show', society %></td>
        <td><%= link_to 'Edit', edit_society_path(society) %></td>
        <td><%= link_to 'Destroy', society, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Society', new_society_path %>
